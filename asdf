[32m+[m[32m    public static class MetodosExtensao[m
[32m+[m[32m    {[m
[32m+[m[32m        public static string CaixaAltaPrimeiraLetra(this string valor)[m
[32m+[m[32m        {[m
[32m+[m[32m            // P√µe a primeira letra em caixa alta[m
[32m+[m[32m            if (valor.Length > 0)[m
[32m+[m[32m            {[m
[32m+[m[32m                char[] array = valor.ToCharArray();[m
[32m+[m[32m                array[0] = char.ToUpper(array[0]);[m
[32m+[m[32m                return new string(array);[m
[32m+[m[32m            }[m
[32m+[m[32m            return valor;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public static int ContaPalavras(this String texto)[m
[32m+[m[32m        {[m
[32m+[m[32m            //conta quantas palavras exite na string[m
[32m+[m[32m            return texto.Split(new char[] { ' ', '.', '?' }, StringSplitOptions.RemoveEmptyEntries).Length;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        //m√©todo com dois par√¢metros[m
[32m+[m[32m        //como o m√©todo vai operar sobre inteiros[m[41m [m
[32m+[m[32m        //o primeiro parametro deve ser um int[m
[32m+[m[32m        public static bool EMaiorQue(this int i, int valor)[m
[32m+[m[32m        {[m
[32m+[m[32m            return i > valor;[m
[32m+[m[32m        }[m
[32m+[m[32m       public static int PrecoFinalComDesconto(this CalculaPreco obj, int quantidade, int custo, int desconto )[m
[32m+[m[32m        {[m
[32m+[m[32m            // calcula o preco com desconto[m
[32m+[m[32m            return (quantidade * custo) * desconto;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public class CalculaPreco[m
[32m+[m[32m    {[m
[32m+[m[32m        public int PrecoTotal(int quantidade, int Custo)[m
[32m+[m[32m        {[m
